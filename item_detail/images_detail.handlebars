<div class="detail__inner">
    <div class="detail__media  detail__media--images">
        <div class="detail__media__img-wrapper  js-image-detail-wrapper">
            <a {{#if detail.width}}style="width:{{detail.width}};height:{{detail.height}};top:{{detail.top}};"{{/if}} href="{{image}}" class="detail__media__img-link  js-detail-img  js-image-detail-link" target="_blank">
                <div {{#if detail.width}}style="width:{{detail.width}};height:{{detail.height}};top:{{detail.top}};"{{/if}} class="detail__media__img-bg  js-detail-img  js-detail-img-bg"></div>
                <img src="{{imageProxy thumbnail}}" {{#if detail.width}}style="width:{{detail.width}};height:{{detail.height}};top:{{detail.top}};"{{/if}} class="detail__media__img-thumbnail  js-detail-img  js-detail-img-thumb" />
                <img {{#if detail.width}}style="width:{{detail.width}};height:{{detail.height}};top:{{detail.top}};"{{/if}} alt="{{title}}" class="detail__media__img-highres  js-detail-img  js-detail-img-high" />
            </a>
        </div>
    </div>
    <div class="detail__body  detail__body--images">
        <div class="c-detail  detail__body__content">
            <h5 class="c-detail__title"><a href="{{url}}">{{title}}</a></h5>
            <div class="c-detail__desc">
                <p><a href="{{url}}">{{domain url}}</a></p>
                <div class="c-detail__filemeta">
                    {{width}} &times; {{height}}
                    <span class="sep  c-detail__sep"></span>
                    <a href="{{image}}" target="_blank" class="js-image-detail-link">DDG.Text.VIEW_FILE</a>
                </div>
            </div>
        </div>
    </div>
</div>
