<div class="tile  tile--f  tile--loc">
	<div class="tile--f__main  tile--loc__main">
        <div class="tile__media">
            {{#if image}}
                <div class="tile__media__wrapper">
                    <img src="" data-src="{{imageProxy image}}" alt="{{title}}" class="tile__media__img  js-lazyload" />
                </div>
            {{else}}
                <span class="ddgsi tile__media__no-img">@</span>
            {{/if}}
        </div>
		<div class="tile__num">{{num}}</div>
        <div class="tile__body">
            <h6 class="tile__title">{{ellipsis name 33}}</h6>
            {{#if neighborhoodOrCity}}
                <span class="tile__neighborhood">{{neighborhoodOrCity}}</span>
            {{/if}}
            <div class="tile__foot  tile__rating  one-line">
                {{#if meta.options.item_footer}}
                    {{{include meta.options.item_footer}}}
                {{else}}
                    {{#if rating}}
                        {{{starRating rating}}}
                    {{/if}}
                    {{#if reviews}}
                        {{{reviewCount reviews '' true}}}
                    {{/if}}
                {{/if}}
            </div>
        </div>
    </div>
    <div class="tile--f__alt  tile--loc__alt">
        <div class="tile__body  {{#and url meta.sourceName}}has-foot{{/and}}">
            <h6 class="tile__title">
                <a href="{{url}}" title="{{name}}">{{ellipsis name 33}}</a>
            </h6>
            {{#if price}}
                <span class="tile__price">{{{priceSymbols price 4}}}</span>
            {{/if}}
            {{#if address_lines}}
                <a class="tile__address" href="{{directions}}">
                    {{#each address_lines}}
                        {{.}}<br />
                    {{/each}}
                </a>
            {{else}}
                {{#if address}}
                    <span class="tile__address">{{ellipsis address 65}}</span>
                {{/if}}
            {{/if}}
            {{#if phone}}
                <a class="tile__phone" href="tel:{{phone}}">{{phone}}</a>
            {{/if}}
            {{#and url meta.sourceName}}
                <div class="tile__foot">
                    {{{moreAt url meta.sourceName dynamicMoreAtText=5 className="tile--loc__more" iconClassName="tile--loc__more__icon" iconPlaceholder="true"}}}
                </div>
            {{/and}}
        </div>
    </div>
</div>
